{
	"presets": [
		"@babel/preset-env"
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		[
			"@babel/transform-runtime"
		],
		[
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
					"@config": "./config",
					"@db": "./db",
					"@loaders": "./loaders",
					"@routes": "./routes",
					"@graphql": "./graphql"
        }
      }
    ]
	]
}